    "architecture": {
        "64bit": {
            "url": "https://github.com/godotengine/godot-builds/releases/download/3.7-dev1/Godot_v3.7-dev1_win64.exe.zip",
            "hash": "sha512:4a573d9024bd577b315310badba0488fd51c296eb7770d71a77020fb62015e6dd18bae2d02ef44800e1259bc0e553022392c4b06601b7890dc4806d86b9a7fff"
        },
        "32bit": {
