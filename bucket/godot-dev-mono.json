        "32bit": {
            "url": "https://github.com/godotengine/godot-builds/releases/download/4.5-dev2/Godot_v4.5-dev2_mono_win32.zip",
            "hash": "sha512:c4128c505fe6fd4ee584e5dd879cd31ab0cb5a4a0e360718fca23f9ecf8b6a8a945706fda972b8d2a19a99397fc6656b98dd4a3e4d6c216f350d8198fe2bde1e",
