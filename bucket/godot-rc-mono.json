            "hash": "sha512:cfb7ccbd207f2df9e99dcacc8214109b98a9971f241d1d527e278914aef7d5c57f851a8e4e586f36478a3a340950f5da1f5fb718f638bcd80ed954d29d7844c3",
            "extract_dir": "Godot_v4.6-rc1_mono_win32"
